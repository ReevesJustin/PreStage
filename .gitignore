# .gitignore

###############################################################################
# Project instantiation / execution state (must never be committed)
###############################################################################
projects/
execution/
runs/
outputs/
artifacts/
sessions/
tmp/
temp/
scratch/

###############################################################################
# OpenCode / agent tooling artifacts
###############################################################################
.opencode/
.opencode-cache/
.opencode-state/
.agent-cache/
.agent-state/

###############################################################################
# Environments
###############################################################################
.env
.env.*
.venv/
venv/
node_modules/

###############################################################################
# Caches / bytecode
###############################################################################
.cache/
__pycache__/
*.pyc
*.pyo

###############################################################################
# Build artifacts
###############################################################################
dist/
build/

###############################################################################
# Logs / temp files
###############################################################################
*.log
logs/
*.tmp

###############################################################################
# Databases
###############################################################################
*.db
*.sqlite

###############################################################################
# Editor / OS
###############################################################################
.vscode/
.idea/
*.swp
*.swo
*~
*.bak
.DS_Store
Thumbs.db
desktop.ini
